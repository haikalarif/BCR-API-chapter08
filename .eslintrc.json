{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "camelcase": "off",
        "no-unused-vars": "off",
        "no-undef": "off",
        "no-extra-boolean-cast": "off",
        "no-extra-semi": "off"
    }
}